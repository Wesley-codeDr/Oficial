		"@vitest/coverage-v8": "^2.1.0",
		"@vitest/ui": "^2.1.0",
		"autoprefixer": "^10.4.0",
		"dotenv": "^17.2.3",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
